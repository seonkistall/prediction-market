    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "sql.js": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "siwe": "^2.1.4",
    "socket.io": "^4.6.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1"
